{
    "name": "crazymeeks/php-validator",
    "description": "A small PHP validator library",
    "keywords": ["PHP", "php", "PHP validator", "PHP validation", "Validator", "PHP validator library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jclaud",
            "email": "jeffclaud17@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/http-foundation": "~3.2"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Crazymeeks\\": "src",
            "Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "crazymeeks/php-validator",
                "version": "v1.0.0",
                "source": {
                    "url": "git://github.com/crazymeeks/php-validator",
                    "type": "git",
                    "reference": "v1.0.0"
                },
                "autoload": {
                    "psr-4": {
                        "Crazymeeks\\": "src"
                    }
                }
            }
            
        }
    ]
}